{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "データ ファクトリ名",
			"defaultValue": "adf4shir"
		},
		"link_vm4shir_test_password": {
			"type": "secureString",
			"metadata": "'link_vm4shir_test' の 'password' のセキュリティで保護された文字列"
		},
		"link_vm_shir_02_password": {
			"type": "secureString",
			"metadata": "'link_vm_shir_02' の 'password' のセキュリティで保護された文字列"
		},
		"link_vm_shir_02_test_password": {
