						"value": "[parameters('link_vm_shir_02_test_properties_typeProperties_host')]"
					},
					"link_vm_shir_02_test_properties_typeProperties_userId": {
						"value": "[parameters('link_vm_shir_02_test_properties_typeProperties_userId')]"
					},
